<h1> Creating New Category </h1>

<%=form_for(@Category)do|f| %>

<div>
    <%=f.label :name %>
    <%=f.text_field :name %>
</div>

<div>
    <%= f.label :description %>
    <%= f.text_field :description %>
</div>

<div>
<%= f.submit "create new category" %>
</div>

<% end %>
