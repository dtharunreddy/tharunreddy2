<h1> Creating New Book </h1>

<%=form_for(@book)do|f| %>

<div>
    <%=f.label :name %>
    <%=f.text_field :name %>
</div>

<div>
    <%= f.label :description %>
    <%= f.text_field :description %>
</div>





<div>
<%= f.submit "create new book" %>
</div>

<% end %>
