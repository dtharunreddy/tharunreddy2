


<h1>Users#edit</h1>

<%= form_for(@user) do |f| %>

<div>
<%=f.label :name %>
<%=f.text_field :name %>
</div>

<div>
<%=f.label :city %>
<%=f.text_field :city %>
</div>

<div>
<%=f.label :country %>
<%=f.text_field :country %>
</div>

<div>
<%=f.label :email %>
<%=f.text_field :email %>
</div>

<%=f.submit %>
<% end %>

<p>
 <%= link_to "Create user",new_user_path %>	
</p>

<p>
<%= link_to "Back", users_path %>
</p>	