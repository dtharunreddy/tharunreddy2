


<h1> Creating New User </h1>

<%=form_for(@user)do|f| %>

<div>
    <%=f.label :name %>
    <%=f.text_field :name %>
</div>

<div>
    <%= f.label :city %>
    <%= f.text_field :city %>
</div>

<div>
    <%= f.label :country %>
    <%= f.text_field :country %>
</div>

<div>
    <%= f.label :email %>
    <%= f.text_field :email %>
</div>



<div>
<%= f.submit "create new user" %>
</div>

<% end %>
